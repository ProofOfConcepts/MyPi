{
    "manifest": {
        "displayName": "Juke Box",
        "invocationName": "Juke Box",
        "category": "PRODUCTIVITY"
    },
    "actions": [
        {
            "name": "com.mypi.action.jukebox_volume",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.mypi.intents.jukebox_volume",
                "parameters": [
                    {
                        "name": "step",
                        "type": "StepIntervals"
                    },
                    {
                        "name": "operation",
                        "type": "VolumeOperations"
                    },
                    {
                        "name": "location",
                        "type": "LocationType"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "jukebox $VolumeOperations:operation volume $StepIntervals:step in $LocationType:location",
                        "jukebox $StepIntervals:step volume $VolumeOperations:operation in $LocationType:location",
                        "in $LocationType:location jukebox $VolumeOperations:operation volume $StepIntervals:step",
                        "in $LocationType:location jukebox $StepIntervals:step volume $VolumeOperations:operation",
                        "in $LocationType:location song $VolumeOperations:operation volume $StepIntervals:step",
                        "in $LocationType:location song $StepIntervals:step volume $VolumeOperations:operation",
                        "in $LocationType:location music $VolumeOperations:operation volume $StepIntervals:step",
                        "in $LocationType:location music $StepIntervals:step volume $VolumeOperations:operation",
                        "jukebox volume $VolumeOperations:operation $StepIntervals:step in $LocationType:location",
                        "jukebox volume $VolumeOperations:operation by $StepIntervals:step in $LocationType:location",
                        "music volume $VolumeOperations:operation $StepIntervals:step in $LocationType:location",
                        "music volume $VolumeOperations:operation by $StepIntervals:step in $LocationType:location",
                        "music volume $StepIntervals:step $VolumeOperations:operation in $LocationType:location",
                        "playback volume $VolumeOperations:operation $StepIntervals:step in $LocationType:location",
                        "playback volume $VolumeOperations:operation by $StepIntervals:step in $LocationType:location",
                        "playback volume $StepIntervals:step $VolumeOperations:operation in $LocationType:location",
                        "song volume $VolumeOperations:operation $StepIntervals:step in $LocationType:location",
                        "song volume $VolumeOperations:operation by $StepIntervals:step in $LocationType:location",
                        "song volume $StepIntervals:step $VolumeOperations:operation in $LocationType:location",
                        "in $LocationType:location jukebox volume $VolumeOperations:operation $StepIntervals:step",
                        "in $LocationType:location jukebox volume $VolumeOperations:operation by $StepIntervals:step",
                        "in $LocationType:location song volume $VolumeOperations:operation $StepIntervals:step",
                        "in $LocationType:location song volume $VolumeOperations:operation by $StepIntervals:step",
                        "in $LocationType:location music volume $VolumeOperations:operation $StepIntervals:step",
                        "in $LocationType:location music volume $VolumeOperations:operation by $StepIntervals:step"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "jukebox volume $operation.raw in $location.raw by $step.raw"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.mypi.commands.jukebox_volume",
                                    "params": {
                                        "stepKey": "$step",
                                        "opeartionKey" : "$operation",
                                        "locationkey": "$location"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
