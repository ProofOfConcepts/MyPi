{
    "manifest": {
        "displayName": "Juke Box",
        "invocationName": "Juke Box",
        "category": "PRODUCTIVITY"
    },
    "actions": [
        {
            "name": "com.acme.actions.play_jukebox",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.acme.intents.play_jukebox",
                "parameters": [
                    {
                        "name": "number",
                        "type": "SchemaOrg_Number"
                    },
                    {
                        "name": "location",
                        "type": "LocationType"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "jukebox song number $SchemaOrg_Number:number in $LocationType:location"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "jukeboxing song number $number in $location.raw"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.acme.commands.play_jukebox",
                                    "params": {
                                        "number": "$number",
                                        "locationkey": "$location"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        }
    ],
    "types": [
        {
            "name": "$LocationType",
            "entities": [
                {
                    "key": "JUKEBOXLOCATION",
                    "synonyms": [
                        "bedroom",
                        "bed room",
                        "living room",
                        "livingroom",
                        "here",
                        "next room",
                        "this room"
                    ]
                }
            ]
        }
    ]
}